source 'https://github.com/CocoaPods/Specs.git'

target '51车生活' do
    # 集成动画框架
    pod 'pop', '~> 1.0.10'
     # 集成 YYKit 框架
    pod 'YYKit' , '~> 1.0.9'
    # 集成网络框架
    pod 'AFNetworking', '~> 3.1.0'
    # 集成 uitableviewcell 自动计算高度工具
    pod 'UITableView+FDTemplateLayoutCell', '~> 1.6'
    # 集成上／下拉刷新工具
    pod 'MJRefresh', '~> 3.1.12'
    # 集成 masonry 布局框架
    pod 'Masonry', '~> 1.0.2'
    # 集成 hud 显示
    pod 'MBProgressHUD', '~> 1.0.0'
    # 集成微信(精简版0.2M)
    pod 'UMengUShare/Social/ReducedWeChat' , '~> 6.4.5'
    # 精简版新浪微博(精简版1M)
    pod 'UMengUShare/Social/ReducedSina', '~> 6.4.5'
    # 集成QQ/QZone/TIM(精简版0.5M)
    pod 'UMengUShare/Social/ReducedQQ' , '~> 6.4.5'
    # 集成钉钉
    pod 'UMengUShare/Social/DingDing' , '~> 6.4.5'
    # 集成邮件
    pod 'UMengUShare/Social/Email', '~> 6.4.5'
    # 集成短信
    pod 'UMengUShare/Social/SMS','~> 6.4.5'
    # 集成极光推送
    pod 'JPush', '~> 3.0'
end
